# Production Environment Configuration for Scout Analytics ETL
# Copy to .env.production and fill in actual values

# Supabase Configuration
SUPABASE_PROJECT_REF=cxzllzyxwpyptfretryc
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here
SUPABASE_DB_URL=postgresql://postgres.cxzllzyxwpyptfretryc:POSTGRES_PASSWORD@aws-0-ap-southeast-1.pooler.supabase.com:6543/postgres
POSTGRES_PASSWORD=your_postgres_password_here

# Temporal Configuration
TEMPORAL_HOST=localhost
TEMPORAL_PORT=7233
TEMPORAL_NAMESPACE=scout-analytics
TEMPORAL_TASK_QUEUE=scout-etl-queue

# OpenTelemetry Configuration
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317
OTEL_EXPORTER_OTLP_INSECURE=true
OTEL_SERVICE_NAME=scout-etl-pipeline
OTEL_SERVICE_VERSION=1.0.0

# Prometheus Configuration
PROMETHEUS_GATEWAY=http://localhost:9090
PROMETHEUS_REMOTE_WRITE_ENDPOINT=https://your-prometheus-remote-write-url
PROMETHEUS_API_KEY=your_prometheus_api_key_here

# Jaeger Configuration
JAEGER_ENDPOINT=http://localhost:14250

# Great Expectations Configuration
GE_CONFIG_DIR=/app/etl/quality
GE_DATA_DOCS_SITE_NAME=local_site

# dbt Configuration
DBT_PROFILES_DIR=/app/dbt-scout
DBT_TARGET=prod
DBT_PROJECT_DIR=/app/dbt-scout

# Bruno Executor Configuration
BRUNO_LOG_LEVEL=INFO
BRUNO_METRICS_PORT=8080
BRUNO_HEALTH_CHECK_PORT=8081
BRUNO_MAX_WORKERS=4
BRUNO_BATCH_SIZE=1000

# Security Configuration
PII_MASKING_ENABLED=true
ENCRYPTION_KEY=your_32_character_encryption_key_here
AUDIT_LOG_LEVEL=INFO

# Data Quality Configuration
QUALITY_THRESHOLD_ERROR=0.95
QUALITY_THRESHOLD_WARNING=0.90
QUALITY_VALIDATION_ENABLED=true

# Performance Configuration
MAX_PARALLEL_WORKFLOWS=5
WORKFLOW_TIMEOUT_MINUTES=60
RETRY_MAX_ATTEMPTS=3
RETRY_INITIAL_INTERVAL_SECONDS=30

# Monitoring Configuration
METRICS_ENABLED=true
TRACING_ENABLED=true
HEALTH_CHECK_ENABLED=true
ALERT_ON_FAILURE=true

# Slack Notifications (Optional)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
SLACK_CHANNEL=#data-alerts
SLACK_NOTIFY_ON_FAILURE=true

# Email Notifications (Optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your_email@gmail.com
SMTP_PASSWORD=your_app_password_here
EMAIL_NOTIFY_ON_FAILURE=true
EMAIL_RECIPIENTS=admin@tbwa.com,data-team@tbwa.com