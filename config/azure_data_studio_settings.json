        "sizex": 2,
        "sizey": 1
      },
      "widget": {
        "insights-widget": {
          "type": {
            "count": {
              "dataDirection": "vertical",
              "dataType": "number",
              "legendPosition": "none",
              "labelFirstColumn": false,
              "columnsAsLabels": false
