# Agent Savage Environment Configuration
# Copy to .env and fill in your values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_ANON_KEY=your-anon-key

# Agent Configuration
SAVAGE_PORT=8000
LOG_LEVEL=info

# Redis Cache (optional)
REDIS_URL=redis://localhost:6379
REDIS_CACHE_TTL=3600

# Storage Configuration
STORAGE_BUCKET=patterns
STORAGE_PUBLIC_URL=https://your-project.supabase.co/storage/v1/object/public

# AI/ML Configuration (optional, for future features)
OPENAI_API_KEY=your-openai-key
PATTERN_AI_ENDPOINT=https://pattern-ai.example.com

# Brand Presets
DEFAULT_ORG_TYPE=corporate
DEFAULT_PRIMARY_COLOR=#2C3E50
DEFAULT_SECONDARY_COLOR=#ECF0F1

# Performance Settings
MAX_CONCURRENT_RENDERS=100
SVG_RENDER_TIMEOUT_MS=500
GIF_ENCODE_TIMEOUT_MS=2000