{
  "version": "1.0",
  "name": "Executive Dashboard",
  "description": "Strategic overview with key performance indicators",
  "pages": [
    {
      "name": "Overview",
      "displayName": "Executive Overview",
      "ordinal": 0,
      "width": 1280,
      "height": 720,
      "config": {
        "layouts": [
          {
            "id": "kpi-cards",
            "displayName": "KPI Cards",
            "ordinal": 0,
            "position": {
              "x": 20,
              "y": 20,
              "width": 1240,
              "height": 160
            },
            "visualType": "card",
            "config": {
              "values": [
                "Total Sales",
                "Sales YoY Growth %",
                "Gross Margin %",
                "Active Stores"
              ],
              "formatting": {
                "fontSize": 24,
                "fontColor": "#212529",
                "backgroundColor": "#ffffff",
                "border": "1px solid #dee2e6"
              }
            }
          },
          {
            "id": "sales-trend",
            "displayName": "Sales Trend",
            "ordinal": 1,
            "position": {
              "x": 20,
              "y": 200,
              "width": 800,
              "height": 300
            },
            "visualType": "lineChart",
            "config": {
              "x": "Date",
              "y": ["Total Sales", "Sales 12 Month Moving Average"],
              "legend": "top",
              "title": "Sales Performance Trend",
              "formatting": {
                "xAxis": {
                  "title": "Date",
                  "labelFormat": "MMM yyyy"
                },
                "yAxis": {
                  "title": "Sales (₱)",
                  "labelFormat": "₱#,0,.0M"
                }
              }
            }
          },
          {
            "id": "regional-performance",
            "displayName": "Regional Performance",
            "ordinal": 2,
            "position": {
              "x": 840,
              "y": 200,
              "width": 420,
              "height": 300
            },
            "visualType": "donutChart",
            "config": {
              "values": "Total Sales",
              "legend": "dim_store[RegionName]",
              "title": "Sales by Region",
              "formatting": {
                "dataLabels": {
                  "show": true,
                  "labelFormat": "₱#,0,.0M"
                }
              }
            }
          },
          {
            "id": "category-mix",
            "displayName": "Category Performance",
            "ordinal": 3,
            "position": {
              "x": 20,
              "y": 520,
              "width": 400,
              "height": 180
            },
            "visualType": "horizontalBarChart",
            "config": {
              "x": "Total Sales",
              "y": "dim_category[CategoryL1]",
              "title": "Top Categories",
              "topN": 10,
              "formatting": {
                "xAxis": {
                  "title": "Sales (₱)",
                  "labelFormat": "₱#,0,.0M"
                }
              }
            }
          },
          {
            "id": "premium-tobacco-mix",
            "displayName": "Premium & Tobacco Mix",
            "ordinal": 4,
            "position": {
              "x": 440,
              "y": 520,
              "width": 380,
              "height": 180
            },
            "visualType": "stackedColumnChart",
            "config": {
              "x": "dim_date[MonthName]",
              "y": ["Tobacco Sales", "Premium Sales", "Total Sales"],
              "title": "Premium & Tobacco Performance",
              "formatting": {
                "yAxis": {
                  "title": "Sales (₱)",
                  "labelFormat": "₱#,0,.0M"
                }
              }
            }
          },
          {
            "id": "prediction-accuracy",
            "displayName": "Prediction Performance",
            "ordinal": 5,
            "position": {
              "x": 840,
              "y": 520,
              "width": 420,
              "height": 180
            },
            "visualType": "gauge",
            "config": {
              "value": "Prediction Accuracy",
              "title": "Model Accuracy",
              "min": 0,
              "max": 1,
              "target": 0.85,
              "formatting": {
                "valueFormat": "0%",
                "colors": {
                  "minimum": "#dc3545",
                  "center": "#ffc107",
                  "maximum": "#28a745"
                }
              }
            }
          }
        ]
      }
    },
    {
      "name": "Performance",
      "displayName": "Performance Analysis",
      "ordinal": 1,
      "width": 1280,
      "height": 720,
      "config": {
        "layouts": [
          {
            "id": "yoy-comparison",
            "displayName": "Year-over-Year Analysis",
            "ordinal": 0,
            "position": {
              "x": 20,
              "y": 20,
              "width": 1240,
              "height": 340
            },
            "visualType": "clusteredColumnChart",
            "config": {
              "x": "dim_date[MonthName]",
              "y": ["Total Sales", "Sales Last Year"],
              "title": "Sales Comparison - Current vs Previous Year",
              "formatting": {
                "yAxis": {
                  "title": "Sales (₱)",
                  "labelFormat": "₱#,0,.0M"
                }
              }
            }
          },
          {
            "id": "store-performance-matrix",
            "displayName": "Store Performance Matrix",
            "ordinal": 1,
            "position": {
              "x": 20,
              "y": 380,
              "width": 600,
              "height": 320
            },
            "visualType": "scatterChart",
            "config": {
              "x": "Total Sales",
              "y": "Gross Margin %",
              "size": "Total Transactions",
              "legend": "dim_store[RegionName]",
              "title": "Store Performance: Sales vs Margin",
              "formatting": {
                "xAxis": {
                  "title": "Sales (₱)",
                  "labelFormat": "₱#,0,.0M"
                },
                "yAxis": {
                  "title": "Gross Margin %",
                  "labelFormat": "0%"
                }
              }
            }
          },
          {
            "id": "top-performers",
            "displayName": "Top Performing Stores",
            "ordinal": 2,
            "position": {
              "x": 640,
              "y": 380,
              "width": 620,
              "height": 320
            },
            "visualType": "table",
            "config": {
              "columns": [
                "dim_store[StoreName]",
                "dim_store[RegionName]",
                "Total Sales",
                "Sales YoY Growth %",
                "Gross Margin %"
              ],
              "title": "Store Performance Ranking",
              "sorting": {
                "column": "Total Sales",
                "direction": "descending"
              },
              "topN": 15,
              "formatting": {
                "Total Sales": "₱#,0",
                "Sales YoY Growth %": "0.0%",
                "Gross Margin %": "0.0%"
              }
            }
          }
        ]
      }
    }
  ],
  "filters": [
    {
      "table": "dim_date",
      "column": "Date",
      "type": "relativeDateRange",
      "config": {
        "defaultPeriod": "last12Months",
        "allowedPeriods": ["last3Months", "last6Months", "last12Months", "yearToDate"]
      }
    },
    {
      "table": "dim_store",
      "column": "RegionName",
      "type": "multiSelect",
      "config": {
        "allowSelectAll": true,
        "defaultSelection": "all"
      }
    }
  ],
  "theme": "scout-theme",
  "annotations": {
    "lastRefresh": "Auto",
    "dataSource": "Scout Lakehouse",
    "owner": "Data Team",
    "accessLevel": "Executive"
  }
}